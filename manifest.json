{
  "name": "Waifu",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Waifu Home Page",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_title": "Waifu",
    "default_popup": "src/page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/index.html"
  },  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["scripts/other.js"]
  }
],
  "permissions": [
    "notifications",
    "tabs"
  ],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["scripts/replace.js"]
  }
],
  "permissions": [
    "notifications",
    "tabs"
  ]
}